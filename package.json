{
  "name": "ai-resume-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "pages:build": "opennextjs-cloudflare build -c wrangler.worker.toml && node scripts/prepare-pages-output.mjs",
    "cf:build": "opennextjs-cloudflare build -c wrangler.worker.toml",
    "cf:preview": "opennextjs-cloudflare build -c wrangler.worker.toml && opennextjs-cloudflare preview -c wrangler.worker.toml",
    "cf:deploy": "opennextjs-cloudflare build -c wrangler.worker.toml && opennextjs-cloudflare deploy -c wrangler.worker.toml --env production",
    "cf:typegen": "wrangler types -c wrangler.worker.toml --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.9.0",
    "ai": "^6.0.86",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.564.0",
    "nes.css": "^2.2.1",
    "next": "16.1.6",
    "openai": "^6.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.65.0"
  }
}
